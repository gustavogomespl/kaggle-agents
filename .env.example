# Optional - LangSmith tracing
LANGSMITH_API_KEY=your_langsmith_api_key
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=kaggle-agents

# Optional - Model configuration
LLM_MODEL=gpt-4-turbo-preview
